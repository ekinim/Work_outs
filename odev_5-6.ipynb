   "source": [
    "SELECT COUNT(*) AS Miktar,\n",
    "\n",
    "CASE\n",
    "\tWHEN Miktar>20 THEN 'Büyük'\n",
    "    WHEN Miktar>10 THEN 'Orta'\n",
    "    ELSE 'Küçük'\n",
    "END AS Buyukluk\n",
    "\n",
    "FROM (\n",
    "\tSELECT \n",
    "        country AS Ulke,\n",
    "\t\tCOUNT(*) AS Miktar\n",
    "    \n",
    "\tFROM customers\n",
    "\n",
    "\tGROUP BY country\n",
    "\n",
    "\tORDER BY Miktar DESC\n",
    ") alt_tablo\n",
    "\n",
    "GROUP BY Buyukluk"
   ]
  },
